# ============================================================================
# Activated features
# ============================================================================

FEATURES=

# ============================================================================
# Architecture dependant things
# ============================================================================

ARCHITECTURE=-DARCH_IS_GENERIC
BUS=-DARCH_IS_32BIT
ENDIANNESS=-DARCH_IS_LITTLE_ENDIAN
SHARED_EXTENSION=so
STATIC_EXTENSION=a
OBJECT_EXTENSION=o

# ============================================================================
# Compiler
# ============================================================================

CC=/opt/toolchains/dc/sh-elf/bin/sh-elf-gcc
SPECIFIC_CFLAGS= 
ALTIVEC_CFLAGS=
CFLAGS=-O2 -fomit-frame-pointer -ml -m4-single-only -ffunction-sections -fdata-sections  -I/usr/local/dc/kos/kos/include -I/usr/local/dc/kos/kos/kernel/arch/dreamcast/include -I/usr/local/dc/kos/kos/addons/include -I/usr/local/dc/kos/kos/../kos-ports/include -D_arch_dreamcast -D_arch_sub_pristine -Wall -g -fno-builtin -I/usr/local/dc/kos/kos/ds/include

# ============================================================================
# Assembler
# ============================================================================

AS=
AFLAGS=
ASSEMBLY_EXTENSION=

# ============================================================================
# Linker
# ============================================================================

SPECIFIC_LDFLAGS= 
API_MAJOR=4
API_MINOR=3
RANLIB=sh-elf-ranlib
AR=sh-elf-ar

# ============================================================================
# Installation
# ============================================================================

INSTALL=/usr/bin/install -c
DESTDIR=
prefix=/usr/local
exec_prefix=${prefix}
libdir=${exec_prefix}/lib
includedir=${prefix}/include

# ============================================================================
# Sources
# ============================================================================

GENERIC_SOURCES=$(SRC_GENERIC)
ASSEMBLY_SOURCES=$()
DCT_IA64_SOURCES=$()
PPC_ALTIVEC_SOURCES=$()
GENERIC_OBJECTS=$(SRC_GENERIC:.c=.o)
ASSEMBLY_OBJECTS=$(:.=.o)
DCT_IA64_OBJECTS=$(:.=.o)
PPC_ALTIVEC_OBJECTS=$(:.c=.o)

STATIC_LIB=libxvidcore.$(STATIC_EXTENSION)
SHARED_LIB=libxvidcore.$(SHARED_EXTENSION)
PRE_SHARED_LIB=libxvidcore.$(SHARED_EXTENSION)
